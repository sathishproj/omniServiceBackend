# .env

# MongoDB connection string (update with your username, password, cluster, and database)
MONGO_URI=mongodb+srv://<username>:<password>@<cluster-url>/<database>?retryWrites=true&w=majority


# CORS allowed origins (comma separated if needed)
CORS_ORIGINS=http://localhost:4200,https://omni-project-gamma.vercel.app

# Application port (optional, defaults to 3000 in your code)
PORT=3000

